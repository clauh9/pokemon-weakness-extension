{
    "name": "Pokemon Weakness",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Find the weaknesses of selected Pokemon",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ]
  }
  