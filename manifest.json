{
    "name": "Pokemon Weakness",
    "version": "2023.05.11",
    "author": "Claudia SA",
    "manifest_version": 3,
    "description": "Lazy overload",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "action": {
      "default_icon": "icon.png",
      "default_popup": "index.html",
      "default_title": "Pokemon Weaknesses"
  }
}
  