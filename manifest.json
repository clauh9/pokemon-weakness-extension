{
    "name": "Pokemon Weakness",
    "version": "2023.05.07",
    "manifest_version": 3,
    "description": "Find the weaknesses of selected Pokemon",
    "permissions": ["activeTab"],
    "host_permissions": ["<all_urls>"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "css": ["style.css"]
      }
    ],
    "action": {
      "default_icon": 
        "icon.png"
      
  }
}
  